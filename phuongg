
public class phuongg {

    int id;
    String name;

    Bai8_48(int id, String name) {
        id = id;
        name = name;
    }

    void display() {
        System.out.println(id + " " + name);}

    public static void main(String[] args) {
        phuongg s1 = new phuongg(111,"Karan");
        phuongg s2 = new phuongg(321,"Aryan");
        s1.display();
        s2.display();
    }
}
